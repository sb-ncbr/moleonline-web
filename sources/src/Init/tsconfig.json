{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "lib": [ "es6", "dom", "esnext.asynciterable", "es2016" ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "ignoreDeprecations": "5.0",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": false,
    "declaration": false,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "types": ["node"],
    "skipDefaultLibCheck": true
  },
  "files": [

    "../../config/common.ts",

    "../FetchingIf.ts",
    "../SimpleRouter.ts",
    "../MoleAPIService.ts",
    "../PatternQueryAPIService.ts",

    //Data Interface
    "../DataInterface.ts",

    "../Bridge.ts",
    "../DataProxy.ts",

    //Router
    "../Common/Util/Router.ts", 

    //Cookies
    "../Common/Util/Cookies.ts",
    
    //LastNSessions
    "../Common/Util/LastNSessions.ts",

    //Common UI components
    "../Common/UI/Tabs/BootstrapTabs.tsx",
    
    //Alert messages
    "../Common/UI/AlertMessages/UI.tsx",

    //InitForm
    "InitForm/UI.tsx",

    //Version strip
    "VersionStrip/UI.tsx",

    "HomePage/Examples.tsx",

    //App
    "App.tsx"
  ]
}
